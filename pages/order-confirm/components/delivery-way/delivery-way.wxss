.order-item{
  border-top: 1rpx solid #EEEEEE;
  display: flex;
  justify-content: space-between;
  padding-top: 25rpx;
  padding-bottom: 25rpx;
}
.order-item_icon{
  width: 48rpx;
  height: 48rpx;
  margin-right: 16rpx;
}
.order-item_key{
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #919191;
  letter-spacing: 0;
  line-height: 40rpx;
}
.order-item_value{
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #262626;
  text-align: right;
  line-height: 32rpx;
}
