  padding-bottom: 16rpx;
  padding-top: 16rpx;
  background-color: #FFFFFF;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #262626;
  letter-spacing: 0;
}
.doctors_body{
  background-color: #FFFFFF;
}
.doctor{
  margin-left: 24rpx;
  margin-right: 24rpx;
  padding-top: 24rpx;
  padding-bottom: 24rpx;
  border-top: 1rpx solid #EEEEEE;

  display: flex;
  align-items: center;
}

.doctor_logo{
  width: 100rpx;
  height: 100rpx;
  /*border-radius: 50rpx;*/
}

.doctor_intro{
  flex: 1;
  display: flex;
