.order{
  background-color: #FFFFFF;
  padding-left: 24rpx;
  padding-right: 24rpx;
}
.order-item{
  border-top: 1rpx solid #EEEEEE;
  display: flex;
  justify-content: space-between;
  padding-top: 25rpx;
  padding-bottom: 25rpx;
}
.order-item_key{
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #919191;
  letter-spacing: 0;
  line-height: 40rpx;
}
.order-item_value{
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #262626;
  text-align: right;
  line-height: 32rpx;
}
.content_main{
  display: flex;
  padding-top: 23rpx;
  padding-bottom: 58rpx;
  padding-left: 24rpx;
  padding-right: 24rpx;
  background-color: #FFFFFF;
  margin-top: 16rpx;
}
.content_bar{
  display: flex;
  justify-content: flex-end;
  padding-bottom: 28rpx;
}
.main_main{
  display: flex;
  flex: 1;
}

.main_bar{
  display: flex;
  align-items: flex-end;
}

.menu-logo{
  width: 144rpx;
  height: 144rpx;
}
.menu-intro{
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding-left: 20rpx;
}
.menu-intro_price{
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #FE0000;
}
