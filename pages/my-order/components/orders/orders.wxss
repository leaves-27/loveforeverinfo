.items{

}
.item{
  padding-left: 24rpx;
  padding-right: 24rpx;
  background-color: #FFFFFF;
  margin-bottom: 16rpx;
}
.item_header{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 72rpx;
  margin-bottom: 22rpx;
}
.item_content{
  display: flex;
  flex-direction: column;
}
.item_footer{
  border-top: 1rpx solid #F1F1F1;
  height: 104rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.content_main{
  display: flex;
  padding-bottom: 58rpx;
}
.content_bar{
  display: flex;
  justify-content: flex-end;
  padding-bottom: 28rpx;
}

.main_main{
  display: flex;
  flex: 1;
}

.main_bar{
  display: flex;
  align-items: flex-end;
}

.menu-logo{
  width: 144rpx;
  height: 144rpx;
}
.menu-intro{
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding-left: 20rpx;
}
.menu-intro_price{
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #FE0000;
}

input[type="text"],input[type="password"],textarea,button{
  outline-style: none;
  -webkit-appearance: none;
  padding: 0;
  padding: 0;
  margin: 0;
  margin: 0;
  outline: none;
}
button::after {
  border: none;
}
.button{
  display: inline-block;
  padding-left: 30rpx;
  padding-right: 30rpx;
  height: 56rpx;
  line-height: 56rpx;

  border-radius: 4rpx;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  text-align: center;
}
.button--default{
  background-image: none;
  background-color: #FFFFFF;
  border: 2rpx solid #DDDDDD;
  color: #1E1E1E;
}
.button--red{
  background-image: linear-gradient(-140deg, #FF6060 0%, #FE0000 100%);
  color: #FFFFFF;
  border: none;
}

.button-interval--small{
  margin-left: 16rpx;
}

.fee{
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
}

.fee--default{
  color: #7F7F7F;
}
.fee--strong{
  color: #262626;
}

